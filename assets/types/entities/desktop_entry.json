{
  "name": "desktop_entry",
  "group": "desktop",
  "description": "Desktop Entry",
  "components": [
    "named",
    "flow_2d",
    "flow_3d"
  ],
  "properties": [
    {
      "name": "appid",
      "data_type": "string",
      "socket_type": "output"
    },
    {
      "name": "source",
      "data_type": "string",
      "socket_type": "output"
    },
    {
      "name": "path",
      "data_type": "string",
      "socket_type": "output"
    },
    {
      "name": "categories",
      "data_type": "array",
      "socket_type": "input"
    },
    {
      "name": "groups",
      "data_type": "array",
      "socket_type": "input"
    },
    {
      "name": "exec",
      "data_type": "string",
      "socket_type": "output"
    },
    {
      "name": "icon",
      "data_type": "string",
      "socket_type": "output"
    },
    {
      "name": "type",
      "data_type": "string",
      "socket_type": "output"
    },
    {
      "name": "comment",
      "data_type": "string",
      "socket_type": "output"
    },
    {
      "name": "keywords",
      "data_type": "array",
      "socket_type": "output"
    },
    {
      "name": "mime_type",
      "data_type": "array",
      "socket_type": "output"
    },
    {
      "name": "terminal",
      "data_type": "bool",
      "socket_type": "output"
    },
    {
      "name": "no_display",
      "data_type": "bool",
      "socket_type": "output"
    },
    {
      "name": "startup_notify",
      "data_type": "bool",
      "socket_type": "output"
    }
  ],
  "extensions": [
    {
      "name": "palette",
      "extension": {
        "content": "Desktop Entry",
        "styles":  {
          "font-size": "12px",
          "font-family": "Fira Code",
          "padding": "5px"
        }
      }
    },
    {
      "name": "shape",
      "extension": {
        "width": 200,
        "socket": {
          "width": 60,
          "height": 30,
          "offset": 5
        },
        "offset": {
          "top": "socket.height",
          "bottom": "socket.height"
        },
        "elements": {
          "title": {
            "show": true,
            "type": "text",
            "content": "element.description",
            "position": {
              "left": 0,
              "top": 0,
              "width": "shape.width",
              "height": "socket.height"
            },
            "styles": {
              "font-size": "12px",
              "fill": "black"
            }
          },
          "symbol": {
            "show": true,
            "type": "text",
            "content": "Desktop Entry",
            "position": {
              "left": 0,
              "top": 0,
              "width": "shape.width",
              "height": "shape.height"
            },
            "styles": {
              "font-family": "Fira Code",
              "font-size": "40px",
              "fill": "fuchsia"
            }
          },
          "id": {
            "show": true,
            "type": "text",
            "content": "shape.id",
            "position": {
              "left": 0,
              "top": "shape.height-socket.height",
              "width": "shape.width",
              "height": "socket.height"
            },
            "styles": {
              "font-size": "9px",
              "fill": "black"
            }
          }
        }
      }
    },
    {
      "name": "dublin-core",
      "extension":{
        "title": "Desktop Entry",
        "subject": "Desktop Entry",
        "creator": "Hanack"
      }
    }
  ]
}
